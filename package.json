{
  "$schema": "https://json.schemastore.org/package",
  "name": "leetabit.dgtiles",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=23.6.0 <24.0.0"
  },
  "scripts": {
    "lint": "tsc --build && cspell . --no-exit-code && prettier . --check && eslint",
    "lint:break": "tsc --build && cspell . && prettier . --check && eslint --max-warnings=0",
    "lint:fix": "prettier . --write && eslint --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "vite",
    "clean": "rm -rf ./dist",
    "build": "vite build",
    "preview": "yarn build && vite preview",
    "verify": "yarn clean && yarn lint:break && yarn test:run && yarn build",
    "update": "yarn upgrade --latest --exact"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@actions/github": "6.0.0",
    "@eslint/js": "9.26.0",
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.3",
    "@vitejs/plugin-react-swc": "3.9.0",
    "cspell": "8.19.3",
    "editorconfig": "2.0.1",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-jsdoc": "50.6.11",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
    "minimatch": "10.0.1",
    "prettier": "3.5.3",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1",
    "vite": "6.3.4",
    "vitest": "3.1.2",
    "vitest-axe": "1.0.0-pre.5"
  }
}
