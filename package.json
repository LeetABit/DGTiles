  "scripts": {
    "upgrade": "pnpm update --latest",
    "clean": "rm -rf ./dist",
    "type": "tsc --build || exit 0",
    "type:break": "tsc --build",
    "spell": "cspell . --no-exit-code",
    "spell:break": "cspell .",
    "style": "prettier . --write",
    "style:break": "prettier . --check",
    "lint": "eslint --fix",
    "lint:break": "eslint --max-warnings=0",
    "test": "vitest run",
    "test:watch": "vitest --test-timeout=99999999 --hook-timeout=99999999 --teardown-timeout=99999999",
    "e2e": "playwright test || exit 0",
    "e2e:break": "playwright test",
    "perf": "lhci autorun",
    "build": "vite build",
    "dev": "vite",
    "preview": "pnpm build && vite preview",
    "fix": "pnpm install && pnpm audit -fix && pnpm type && pnpm spell && pnpm style && pnpm lint && pnpm test && pnpm e2e && pnpm perf && pnpm clean && pnpm build",
    "verify": "pnpm install && pnpm audit && pnpm type:break && pnpm spell:break && pnpm style:break && pnpm lint:break && pnpm test && pnpm e2e && pnpm perf && pnpm clean && pnpm build"
  },
  "dependencies": {
