{
  "$schema": "https://json.schemastore.org/package",
  "name": "leetabit.dgtiles",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "tsc --build && eslint",
    "fix": "eslint --fix",
    "test": "vitest",
    "dev": "vite",
    "build": "rm -rf ./dist && vite build",
    "preview": "rm -rf ./dist && vite build && vite preview",
    "verify": "rm -rf ./dist && tsc --build && eslint --max-warnings=0 && vitest run && vite build",
    "update": "yarn upgrade --latest --exact"
  },
  "devDependencies": {
    "@actions/github": "6.0.0",
    "@eslint/js": "9.24.0",
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/node": "22.14.0",
    "editorconfig": "2.0.1",
    "eslint": "9.24.0",
    "jiti": "2.4.2",
    "minimatch": "10.0.1",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vite": "6.2.5",
    "vitest": "3.1.1"
  }
}
