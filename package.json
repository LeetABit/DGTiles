{
  "$schema": "https://json.schemastore.org/package",
  "name": "leetabit.dgtiles",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.0.0 <25.0.0"
  },
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "lint": "tsc --build && cspell . --no-exit-code && prettier . --check && eslint",
    "lint:break": "tsc --build && cspell . && prettier . --check && eslint --max-warnings=0",
    "lint:fix": "prettier . --write && eslint --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "vite",
    "clean": "rm -rf ./dist",
    "build": "vite build",
    "preview": "pnpm build && vite preview",
    "verify": "pnpm clean && pnpm lint:break && pnpm test:run && pnpm build",
    "upgrade": "pnpm update --latest"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.12.0"
  },
  "devDependencies": {
    "@actions/github": "7.0.0",
    "@axe-core/react": "4.10.2",
    "@eslint/js": "9.36.0",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react-swc": "4.1.0",
    "cspell": "9.2.1",
    "csstype": "3.2.3",
    "editorconfig": "3.0.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsdoc": "60.3.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jiti": "2.6.0",
    "jsdom": "27.4.0",
    "minimatch": "10.0.3",
    "prettier": "3.8.1",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.11",
    "vitest": "3.2.4",
    "vitest-axe": "1.0.0-pre.5"
  },
  "resolutions": {
    "undici": "6.23.0"
  }
}
